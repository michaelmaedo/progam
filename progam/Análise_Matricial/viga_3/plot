set title "Forca cortante (kN)"
set xlabel "x(m)"
set ylabel "V(kN)"
set yrange [9.072001:-9.072001] noreverse
set term postscript enhanced color; set output "Força cortante da barra 2.eps"
plot V(x) with boxes 3 

